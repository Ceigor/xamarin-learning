<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BareBonesEnterprise.View.QuotesView"
             xmlns:view="clr-namespace:BareBonesEnterprise.View"
             xmlns:viewModelBase ="clr-namespace:BareBonesEnterprise.ViewModel.Base"
             viewModelBase:LocatorViewModel.AutowireViewModel="true">
    <ListView x:Name="Quotes" ItemSelected="OnItemSelected" ItemsSource="{Binding Quotes}">
        <ListView.ItemTemplate>
            <DataTemplate>
                <view:QuoteCell/>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>