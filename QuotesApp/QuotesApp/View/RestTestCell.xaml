<?xml version="1.0" encoding="UTF-8"?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuotesApp.View.RestTestCell">
    <ViewCell.View>
        <StackLayout BackgroundColor="Black" Orientation="Vertical" VerticalOptions="FillAndExpand" Padding="{StaticResource CellPadding}">
            <Label Text="{Binding UserId}" Style="{StaticResource DescriptionStyle}" HorizontalOptions="CenterAndExpand" />
            <Label Text="{Binding Id}" Style="{StaticResource DescriptionStyle}" HorizontalOptions="CenterAndExpand" />
            <Label Text="{Binding Title}" Style="{StaticResource TitleStyle}" HorizontalOptions="CenterAndExpand" />
            <Label Text="{Binding Body}" Style="{StaticResource DescriptionStyle}" HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ViewCell.View>
</ViewCell>